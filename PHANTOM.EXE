        valueLabel.ZIndex = 5
        valueLabel.Parent = sliderContainer
        
        -- Slider track background
        local sliderTrack = Instance.new("Frame")
        sliderTrack.Size = UDim2.new(1, -30, 0, 6)
        sliderTrack.Position = UDim2.new(0, 15, 0, 30)
